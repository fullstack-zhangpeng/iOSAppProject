    pod 'YYModel'
    # UM
    #pod 'UMCCommon'
    #pod 'UMCPush'
    
    target 'iOSAppProjectTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
    target 'iOSAppProjectUITests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end
